{
  "terminal": {
    "dock": "right"
  },
  "git_panel": {
    "dock": "right"
  },
  "ui_font_size": 20,
  "buffer_font_size": 20,
  "theme": {
    "mode": "light",
    "light": "Gruvbox Light Soft",
    "dark": "Gruvbox Dark Soft"
  },
  "base_keymap": "Emacs",
  "cursor_shape": "block",
  "cursor_blink": false,
  "buffer_font_family": "Iosevka Slab",
  "ui_font_family": "Atkinson Hyperlegible",
  "soft_wrap": "editor_width",
  "gutter": {
    "line_numbers": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "indent_guides": {
    "enabled": false
  }
}
